%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!55 &1
PhysicsManager:
  # Editor visibility and hide flags
  m_ObjectHideFlags: 0
  # Serialized version of this configuration
  serializedVersion: 14
  # Gravity vector applied globally
  m_Gravity: {x: 0, y: -9.81, z: 0}
  # Reference to the default physics material (0 if none)
  m_DefaultMaterial: {fileID: 0}
  # Threshold above which collisions will produce bounces
  m_BounceThreshold: 2
  # Maximum velocity allowed for depenetration (to avoid jitter)
  m_DefaultMaxDepenetrationVelocity: 10
  # Threshold below which objects go to sleep to save performance
  m_SleepThreshold: 0.001
  # Default offset used to prevent interpenetration of colliders
  m_DefaultContactOffset: 0.01
  # Iterations for the physics solver (improves accuracy)
  m_DefaultSolverIterations: 6
  m_DefaultSolverVelocityIterations: 1
  # Whether raycasts and other queries hit backfaces (0 = no, 1 = yes)
  m_QueriesHitBackfaces: 0
  # Whether trigger colliders are included in physics queries
  m_QueriesHitTriggers: 1
  # Enables adaptive force application (0 = disabled)
  m_EnableAdaptiveForce: 0
  # Cloth inter-collision distance and stiffness (unused by default)
  m_ClothInterCollisionDistance: 0
  m_ClothInterCollisionStiffness: 0
  # Collision contacts generation mode (1 = default)
  m_ContactsGeneration: 1
  # Collision layer matrix (bitmask string)
  m_LayerCollisionMatrix: ffefffffffefffffffefffffffffffffffefffffffebe3ffffffffffffffffffffefffffffebfbffdfedffffffe7ffffc8e0ffffffffffffffffffffffffffffffffffffffffffffdffdffffdfffffffdfffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffffff
  # Simulation mode (0 = discrete; other values may denote continuous modes)
  m_SimulationMode: 0
  # Auto-sync transform changes to physics objects (0 = manual sync)
  m_AutoSyncTransforms: 0
  # Reuse collision callback objects to optimize performance
  m_ReuseCollisionCallbacks: 1
  # Whether collision callbacks are invoked
  m_InvokeCollisionCallbacks: 1
  # Cloth-specific inter-collision toggle
  m_ClothInterCollisionSettingsToggle: 0
  # Gravity setting specifically for cloth simulation
  m_ClothGravity: {x: 0, y: -9.81, z: 0}
  # Mode for generating contact pairs (0 = default)
  m_ContactPairsMode: 0
  # Type of broadphase algorithm for collision detection (0 = default)
  m_BroadphaseType: 0
  # World bounds define the simulation area
  m_WorldBounds:
    m_Center: {x: 0, y: 0, z: 0}
    m_Extent: {x: 250, y: 250, z: 250}
  # Subdivisions used in broadphase for performance tuning
  m_WorldSubdivisions: 8
  # Friction computation type (0 = default)
  m_FrictionType: 0
  # Enable enhanced determinism for reproducible simulations (0 = disabled)
  m_EnableEnhancedDeterminism: 0
  # Enable unified heightmaps (1 = enabled)
  m_EnableUnifiedHeightmaps: 1
  # Improved patch friction for terrain interactions (0 = disabled)
  m_ImprovedPatchFriction: 0
  # Solver type (0 = iterative default)
  m_SolverType: 0
  # Maximum angular speed for rigidbodies (to limit rotation speed)
  m_DefaultMaxAngularSpeed: 7
  # Threshold for detecting fast-moving objects
  m_FastMotionThreshold: 3.4028235e+38
  
  # --- Additional Enhancements ---
  # Continuous Collision Detection (CCD) threshold to avoid tunnelling issues.
  m_CCDThreshold: 0.1
  # Enable multithreading for the physics simulation (1 = enabled, 0 = disabled)
  m_UseMultithreading: 1
  # Adaptive solver iterations that can adjust based on scene complexity.
  m_AdaptiveSolverIterations: 8
  # Maximum number of substeps for high-velocity or high-precision scenarios.
  m_MaxSubsteps: 5
  # Time scale for physics simulation (useful for slow motion effects)
  m_PhysicsTimeScale: 1.0
