  tasks:
  - <key>k__BackingField: pumpCover
    <taskText>k__BackingField: Avaa suodattimen pakkaus
    <hint>k__BackingField: "Suodatin on p\xF6yd\xE4ll\xE4 olevista esineist\xE4 isoin.
      Voit avata suodattimen pakkauksen tarttumalla paketista yhdell\xE4 k\xE4della
      ja tarttumalla toisella k\xE4dell\xE4 pakkauksen avauskohdasta. Avauskohta
      hohtaa sinisen\xE4 kun siit\xE4 voi tarttua. Avauskohta sijaitsee paketin p\xE4\xE4ss\xE4."
    <points>k__BackingField: 1
    <timed>k__BackingField: 0
    <timeToCompleteTask>k__BackingField: 0
    <failWhenOutOfTime>k__BackingField: 0
  - <key>k__BackingField: attachFilter
    <taskText>k__BackingField: "Kiinnit\xE4 suodatin pumppuun"
    <hint>k__BackingField: "Voit kiinnitt\xE4\xE4 suodattimen pumppuun viem\xE4ll\xE4
      suodatin pumpun luo ja vapauttamalla tarttumisn\xE4pp\xE4imest\xE4, kun pumpun
      kohdalla n\xE4kyy sininen j\xE4ljitelm\xE4 suodattimesta."
    <points>k__BackingField: 1
    <timed>k__BackingField: 0
    <timeToCompleteTask>k__BackingField: 0
    <failWhenOutOfTime>k__BackingField: 0
  - <key>k__BackingField: fillPepton
    <taskText>k__BackingField: "Siirr\xE4 1 ml peptonivett\xE4 suodattimeen"
    <hint>k__BackingField: "Siirr\xE4 peptonivesi suodattimeen p\xF6yd\xE4ll\xE4
      olevan punavalkoisen pipetin avulla. Siirr\xE4 neste pipettiin ty\xF6nt\xE4m\xE4ll\xE4
      pipetti nesteeseen ja vet\xE4m\xE4ll\xE4 ohjaimen tattia oikealle. Siirr\xE4
      neste pipetist\xE4 suodattimeen vet\xE4m\xE4ll\xE4 tattia vasemmalle."
    <points>k__BackingField: 1
    <timed>k__BackingField: 0
    <timeToCompleteTask>k__BackingField: 0
    <failWhenOutOfTime>k__BackingField: 0
  - <key>k__BackingField: usePump
    <taskText>k__BackingField: "K\xE4ynnist\xE4 pumppu"
    <hint>k__BackingField: "K\xE4ynnist\xE4 pumppu painamalla pumpussa olevaa virtan\xE4pp\xE4int\xE4."
    <points>k__BackingField: 1
    <timed>k__BackingField: 0
    <timeToCompleteTask>k__BackingField: 0
    <failWhenOutOfTime>k__BackingField: 0
  - <key>k__BackingField: fillMedicine
    <taskText>k__BackingField: "Lis\xE4\xE4 0,15 ml l\xE4\xE4kett\xE4 suodattimeen"
    <hint>k__BackingField: "L\xE4\xE4ke on p\xF6yd\xE4ll\xE4 olevassa pieness\xE4
      pullossa, jossa lukee \"Laake\". Siirr\xE4 nestett\xE4 pipettiin vet\xE4m\xE4ll\xE4
      ohjaimen tattia oikealle ja siirr\xE4 nestett\xE4 pois vet\xE4m\xE4ll\xE4 tattia
      vasemmalle"
    <points>k__BackingField: 1
    <timed>k__BackingField: 0
    <timeToCompleteTask>k__BackingField: 0
    <failWhenOutOfTime>k__BackingField: 0
  - <key>k__BackingField: usePumpAgain
    <taskText>k__BackingField: "K\xE4ynnist\xE4 pumppu"
    <hint>k__BackingField: "K\xE4ynnist\xE4 pumppu painamalla pumpussa olevaa virtan\xE4pp\xE4int\xE4."
