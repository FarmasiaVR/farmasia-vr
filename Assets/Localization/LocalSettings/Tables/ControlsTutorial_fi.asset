  m_TableData:
  - m_Id: 1750974070784
    m_Localized: "Teht\xE4v\xE4 suoritettu: "
    m_Metadata:
      m_Items: []
  - m_Id: 2058915676160
    m_Localized: "Avaa j\xE4\xE4kaapin ovi ja pudota kumiankka j\xE4\xE4kaapin sis\xE4lle
      siirty\xE4ksesi seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 2080352763904
    m_Localized: "Pudota kumiankka p\xF6yd\xE4n p\xE4\xE4lle siirty\xE4ksesi seuraavaan
      osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 2098283413504
    m_Localized: "Tartu nurkan takana olevaan kumiankkaan siirty\xE4ksesi seuraavaan
      osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 2119452065792
    m_Localized: "Puhdista p\xF6yd\xE4ll\xE4 oleva kumiankka etanoliruiskulla siirty\xE4ksesi
      seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 2151563657216
    m_Localized: "Siirr\xE4 30ml soija-kaseiinia pienemp\xE4\xE4n pulloon siirty\xE4ksesi
      seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 2178684026880
    m_Localized: "Ota sormenj\xE4lkin\xE4ytteet painamalla soijakaseiinia jollain
      sormella noin viiden sekunnin ajan (4.5 - 5.5 sekunttia), \xE4l\xE4 kuitenkaan
      liian pitk\xE4\xE4n"
    m_Metadata:
      m_Items: []
  - m_Id: 2201660424192
    m_Localized: "L\xE4p\xE4isit tutorialin"
    m_Metadata:
      m_Items: []
  - m_Id: 573918476763136
    m_Localized: Avaa suodattimen pakkaus
    m_Metadata:
      m_Items: []
  - m_Id: 3178477783089152
    m_Localized: "Kiinnit\xE4 suodatin pumppuun"
    m_Metadata:
      m_Items: []
  - m_Id: 3178537296068608
    m_Localized: "Siirr\xE4 1 ml peptonivett\xE4 suodattimeen"
    m_Metadata:
      m_Items: []
  - m_Id: 3178567448920064
    m_Localized: "K\xE4ynnist\xE4 pumppu"
    m_Metadata:
      m_Items: []
  - m_Id: 3178591339675648
    m_Localized: "Lis\xE4\xE4 0,15 ml l\xE4\xE4kett\xE4 suodattimeen"
    m_Metadata:
      m_Items: []
  - m_Id: 3178622180392960
    m_Localized: "K\xE4ynnist\xE4 pumppu"
    m_Metadata:
      m_Items: []
  - m_Id: 3178667881529344
    m_Localized: Pura pumppu
    m_Metadata:
      m_Items: []
  - m_Id: 3178734348664832
    m_Localized: Avaa skalpelli
    m_Metadata:
      m_Items: []
  - m_Id: 3178763394220032
    m_Localized: Leikkaa suodatin kahtia
    m_Metadata:
      m_Items: []
  - m_Id: 3178788006395904
    m_Localized: Avaa pinsettien pakkaus
    m_Metadata:
      m_Items: []
  - m_Id: 3178830670856192
    m_Localized: "Vie suodattimen puolikkaat soijakaseiini- sek\xE4 tioglykoli-pulloihin"
    m_Metadata:
      m_Items: []
  - m_Id: 3184855968907264
    m_Localized: "Ota sormenj\xE4lkin\xE4ytteet painamalla soijakaseiinia jollain
      sormella noin viiden sekunnin ajan (4.5 - 5.5 sekunttia), \xE4l\xE4 kuitenkaan
      liian pitk\xE4\xE4n"
    m_Metadata:
      m_Items: []
  - m_Id: 3185338062209024
    m_Localized: "Avaa j\xE4\xE4kaapin ovi ja pudota kumiankka j\xE4\xE4kaapin sis\xE4lle
      siirty\xE4ksesi seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 3185658154713088
    m_Localized: "Pudota kumiankka p\xF6yd\xE4n p\xE4\xE4lle siirty\xE4ksesi seuraavaan
      osioon."
    m_Metadata:
      m_Items: []
  - m_Id: 3185909980725248
    m_Localized: "Tartu nurkan takana olevaan kumiankkaan siirty\xE4ksesi seuraavaan
      osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 3186257663361024
    m_Localized: "Puhdista p\xF6yd\xE4ll\xE4 oleva kumiankka etanoliruiskulla siirty\xE4ksesi
      seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 3186615945003008
    m_Localized: "Siirry edess\xE4si olevan p\xF6yd\xE4n luo ja seuraa sein\xE4ll\xE4
      olevia ohjeita. Tarvittaessa voit saada vinkin n\xE4kyville tarttumalla sinisest\xE4
      vinkkilaatikosta."
    m_Metadata:
      m_Items: []
  - m_Id: 3186941309747200
    m_Localized: "Avaa p\xF6yd\xE4ll\xE4 olevan pipetink\xE4rki pakkauksesta"
    m_Metadata:
      m_Items: []
  - m_Id: 3187246743158784
    m_Localized: "Kiinnit\xE4 pipetink\xE4rki pipettiin kiinni"
    m_Metadata:
      m_Items: []
  - m_Id: 3187660804849664
    m_Localized: "Siirr\xE4 30ml soija-kaseiinia pienemp\xE4\xE4n pulloon siirty\xE4ksesi
      seuraavaan osioon"
    m_Metadata:
      m_Items: []
  - m_Id: 3188487078543360
    m_Localized: "Hienoa! K\xE4yd\xE4\xE4n viel\xE4 kokonaisuus l\xE4pi harjoittelemalla
      pumpun kasaamista ja k\xE4ytt\xF6\xE4"
    m_Metadata:
      m_Items: []
  - m_Id: 3189827343220736
    m_Localized: "Peliss\xE4 t\xE4ytyy osata siirt\xE4\xE4 nestett\xE4 yhdest\xE4
      pullosta toiseen. Nesteensiirto tapahtuu ty\xF6nt\xE4m\xE4ll\xE4 pipetti nesteeseen
      ja ottamalla nestett\xE4 ty\xF6nt\xE4m\xE4ll\xE4 tattia oikealle tai koskettamalla
      kosketusalustan oikeaa puolta. Nestett\xE4 siirret\xE4\xE4n pipetist\xE4 pulloon
      ty\xF6nt\xE4m\xE4ll\xE4 tattia vasemmalle tai koskettamalla alustan vasenta
      puolta."
    m_Metadata:
      m_Items: []
  - m_Id: 3190311332347904
    m_Localized: "Joitakin esineit\xE4 pystyy k\xE4ytt\xE4m\xE4\xE4n vasta kun ne
      yhdist\xE4\xE4 toiseen esineeseen. Yhdist\xE4\xE4ksesi esineit\xE4 vie esine
      kiinnityskohdan l\xE4helle kunnes sinisell\xE4 n\xE4kyy esineen j\xE4ljenn\xF6s
      ja p\xE4\xE4st\xE4 esineest\xE4 irti."
    m_Metadata:
      m_Items: []
  - m_Id: 3190728980168704
    m_Localized: "Peliss\xE4 on monenlaisia v\xE4lineit\xE4, joita t\xE4ytyy valmistella
      ennen kuin niit\xE4 voi k\xE4ytt\xE4\xE4.\n\nJotkut esineet esimerkiksi t\xE4ytyy
      avata paketista. Avaa esine paketista tarttumalla paketin ei-ter\xE4v\xE4st\xE4
      avauskohdasta. "
    m_Metadata:
      m_Items: []
  - m_Id: 3191143348043776
    m_Localized: "Peliss\xE4 on monia erilaisia esineit\xE4, joilla on erityistoiminnallisuutta
      kun niihin tarttuu. T\xE4t\xE4 erityistoiminnallisuutta voi k\xE4ytt\xE4\xE4
      painamalla ohjaimen liipaisinta."
    m_Metadata:
      m_Items: []
  - m_Id: 3191583422808064
    m_Localized: "Jos haluat siirty\xE4 tiettyyn paikkaan pelin sis\xE4ll\xE4, niin
      kosketa kosketuslevyn alaosaa tai ved\xE4 tattia alasp\xE4in, osoita paikkaan
      jonne haluat siirty\xE4 ja vapauta."
    m_Metadata:
      m_Items: []
  - m_Id: 3191904932986880
    m_Localized: "Esineisiin voi tarttua my\xF6s kaukaa. Ved\xE4 ohjaimen tattia
      eteenp\xE4in tai kosketa ohjaimen kosketusalustan yl\xE4osaa. T\xE4m\xE4n j\xE4lkeen
      osoita tartuttavaan esineeseen osoittimella ja vapauta tatista tai kosketuslevyst\xE4."
    m_Metadata:
      m_Items: []
  - m_Id: 3192629054406656
    m_Localized: "Joidenkin esineiden kanssa voi vuorovaikuttaa painamalla tai t\xF6kk\xE4\xE4m\xE4ll\xE4
      niit\xE4."
    m_Metadata:
      m_Items: []
  - m_Id: 3192945749524480
    m_Localized: "Jos laitat k\xE4tesi esineen luo ja esine muuttuu kirkkaaksi, niin
      esineeseen voi tarttua.\n\nTartu esineeseen painamalla ohjaimen sivulla olevaa
      \"Grab\"-n\xE4pp\xE4int\xE4. Vapauta esine painamalla n\xE4pp\xE4int\xE4 uudelleen."
    m_Metadata:
      m_Items: []
  - m_Id: 3286531144941568
    m_Localized: Vihjeteksti
    m_Metadata:
      m_Items: []
  - m_Id: 3287023933718528
    m_Localized: "Palaa\np\xE4\xE4valikkoon"
    m_Metadata:
      m_Items: []
  - m_Id: 3287396664737792
    m_Localized: Aloita alusta
    m_Metadata:
      m_Items: []
  - m_Id: 3287679566348288
    m_Localized: Avaa
    m_Metadata:
      m_Items: []
  - m_Id: 3288350667571200
    m_Localized: Peptonivesi
    m_Metadata:
      m_Items: []
  - m_Id: 3288744340750336
    m_Localized: Soijakaseiini
    m_Metadata:
      m_Items: []
  - m_Id: 4307450160218112
    m_Localized: "Suodatin on p\xF6yd\xE4ll\xE4 olevista esineist\xE4 isoin. Voit
      avata suodattimen pakkauksen tarttumalla paketista yhdell\xE4 k\xE4della ja
      tarttumalla toisella k\xE4dell\xE4 pakkauksen avauskohdasta. Avauskohta hohtaa
      sinisen\xE4 kun siit\xE4 voi tarttua. Avauskohta sijaitsee paketin p\xE4\xE4ss\xE4."
    m_Metadata:
      m_Items: []
  - m_Id: 4307509555757056
    m_Localized: "Voit kiinnitt\xE4\xE4 suodattimen pumppuun viem\xE4ll\xE4 suodatin
      pumpun luo ja vapauttamalla tarttumisn\xE4pp\xE4imest\xE4, kun pumpun kohdalla
      n\xE4kyy sininen j\xE4ljitelm\xE4 suodattimesta."
    m_Metadata:
      m_Items: []
  - m_Id: 4307562458513408
    m_Localized: "Siirr\xE4 peptonivesi suodattimeen p\xF6yd\xE4ll\xE4 olevan punavalkoisen
      pipetin avulla. Siirr\xE4 neste pipettiin ty\xF6nt\xE4m\xE4ll\xE4 pipetti nesteeseen
      ja vet\xE4m\xE4ll\xE4 ohjaimen tattia oikealle. Siirr\xE4 neste pipetist\xE4
      suodattimeen vet\xE4m\xE4ll\xE4 tattia vasemmalle."
    m_Metadata:
      m_Items: []
  - m_Id: 4307935181144064
    m_Localized: "K\xE4ynnist\xE4 pumppu painamalla pumpussa olevaa virtan\xE4pp\xE4int\xE4."
    m_Metadata:
      m_Items: []
  - m_Id: 4307970690121728
    m_Localized: "L\xE4\xE4ke on p\xF6yd\xE4ll\xE4 olevassa pieness\xE4 pullossa,
      jossa lukee \"Laake\". Siirr\xE4 nestett\xE4 pipettiin vet\xE4m\xE4ll\xE4 ohjaimen
      tattia oikealle ja siirr\xE4 nestett\xE4 pois vet\xE4m\xE4ll\xE4 tattia vasemmalle."
    m_Metadata:
      m_Items: []
  - m_Id: 4308017624383488
    m_Localized: "K\xE4ynnist\xE4 pumppu painamalla pumpussa olevaa virtan\xE4pp\xE4int\xE4."
    m_Metadata:
      m_Items: []
  - m_Id: 4308060926377984
    m_Localized: "Pura pummpu osiin irrottamalla aluksi korkki ja sen j\xE4lkeen
      tarttumalla lasiosa."
    m_Metadata:
      m_Items: []
  - m_Id: 4308250185957376
    m_Localized: "Tartu ensiksi skalpelliin yhdell\xE4 k\xE4dell\xE4 ja t\xE4m\xE4n
      j\xE4lkeen tartu toisella k\xE4dell\xE4 skalpellin avauskohdasta."
    m_Metadata:
      m_Items: []
  - m_Id: 4308363537022976
    m_Localized: "Leikkaa suodatin kahtia t\xF6kk\xE4isem\xE4ll\xE4 suodattimen valkoista
      osaa skalpellilla."
    m_Metadata:
      m_Items: []
  - m_Id: 4308401747132416
    m_Localized: "Avaa pinsettien pakkaus tarttumalla yhdell\xE4 k\xE4dell\xE4 pakkauksesta
      ja toisella k\xE4dell\xE4 pakkauksen avauskohdasta."
    m_Metadata:
      m_Items: []
  - m_Id: 4308455341948928
    m_Localized: Pystyt liikuttamaan suodattimen puolikkaita tarttumalla pinsetteihin
      ja painamalla liipaisinta puolikkaan luona. Pudota yksi puolikas pulloon, jossa
      lukee "Tioglykoli" ja toinen puolikas pulloon, jossa lukee "Soijakaseiini".
    m_Metadata:
      m_Items: []
  - m_Id: 4927973957304320
    m_Localized: "Nykyinen teht\xE4v\xE4"
    m_Metadata:
      m_Items: []
  - m_Id: 4993817798246400
    m_Localized: "Hyv\xE4\xE4 ty\xF6t\xE4!\n\nOlet nyt valmis astumaan ty\xF6skentelyhuoneeseen.\n\nOnnea
      matkaan! :)"
    m_Metadata:
      m_Items: []
